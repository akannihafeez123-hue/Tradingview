{
  "name": "omni-quantum-institutional-trading-system",
  "version": "12.0.0",
  "description": "OMNI QUANTUM INSTITUTIONAL AI - Ultimate Edition Pro Max - Bitget Edition - Quantum-Enhanced Professional Trading System",
  "main": "quantum-trader.js",
  "type": "module",
  "bin": {
    "quantum-trader": "./quantum-trader.js"
  },
  "scripts": {
    "start": "node quantum-trader.js",
    "dev": "node --watch quantum-trader.js",
    "test": "echo \"No tests specified\" && exit 0",
    "setup": "npm install",
    "update": "npm update",
    "clean": "rm -rf node_modules package-lock.json",
    "install-deps": "npm install ws axios moment lodash mathjs dotenv winston node-fetch",
    "docker-build": "docker build -t quantum-trader .",
    "docker-run": "docker run -d --name quantum-trader --restart unless-stopped quantum-trader",
    "docker-logs": "docker logs -f quantum-trader",
    "telegram-test": "node -e \"const {tg} = require('./quantum-trader.js'); tg('sendMessage', {chat_id: process.env.TELEGRAM_CHAT_ID, text: 'Quantum System Test Message'}).then(console.log).catch(console.error);\"",
    "bitget-test": "node -e \"const bitgetAPI = require('./quantum-trader.js').bitgetAPI; bitgetAPI.getTicker('BTCUSDT').then(console.log).catch(console.error);\"",
    "scan": "node quantum-trader.js --scan",
    "backtest": "node quantum-trader.js --backtest",
    "optimize": "node quantum-trader.js --optimize"
  },
  "keywords": [
    "trading",
    "algorithmic-trading",
    "quantum",
    "ai",
    "bitget",
    "cryptocurrency",
    "trading-bot",
    "institutional",
    "trading-system",
    "quantum-computing",
    "machine-learning",
    "neural-networks",
    "crypto",
    "defi",
    "web3"
  ],
  "author": "OMNI QUANTUM INSTITUTIONAL AI TEAM",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "https://github.com/omni-quantum/quantum-trading-system.git"
  },
  "bugs": {
    "url": "https://github.com/omni-quantum/quantum-trading-system/issues"
  },
  "homepage": "https://github.com/omni-quantum/quantum-trading-system#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "ws": "^8.16.0",
    "axios": "^1.6.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "mathjs": "^11.11.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@types/lodash": "^4.14.0",
    "@types/mathjs": "^11.0.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "nodemon": "^3.0.0"
  },
  "optionalDependencies": {
    "better-sqlite3": "^9.0.0"
  },
  "peerDependencies": {
    "node": ">=18.0.0"
  },
  "files": [
    "quantum-trader.js",
    "README.md",
    "LICENSE",
    ".env.example",
    "quantum_state.json",
    "trade_history.json"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "config": {
    "unsafe-perm": true
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
